def is_city_in_telangana(city):
    telangana_cities = ["Hyderabad", "Warangal", "Karimnagar", "Nizamabad", "Khammam", "Adilabad", "Mahbubnagar"]

    if city.capitalize() in telangana_cities:
        return True
    else:
        return False

# Input the city name
city_name = input("Enter the city name: ")

# Check and print the result
if is_city_in_telangana(city_name):
    print(f"{city_name} is in Telangana.")
else:
    print(f"{city_name} is not in Telangana.")
