month = int(input("Enter a month: "))
day = int(input("Enter a day: "))

def season(month):
    if (month == 12 or 1 <= month <= 3):
        print("winter") 
    elif (4 <= month <= 5):
        print("spring")
    elif (6 <= month <= 8):
        print("summer")
    else:
        print("rainy")
    
season(month)
